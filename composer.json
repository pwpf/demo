{
    "name": "pwpf/demo",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Plugin_Name\\App\\": "app/",
            "Plugin_Name\\": ""
        },
        "classmap": [
            "app",
            "includes",
            "vendor_prefixed/"
        ]
    },
    "require": {
    },
    "require-dev": {
        "pwpf/pwpf": "dev-master",
        "humbug/php-scoper": "^0.12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "prefix-dependencies": [
            "composer prefix-pwpf",
            "touch ./vendor_prefixed/dependencies-prefixed.txt",
            "composer du"
        ],
        "prefix-pwpf": [
            "php ./vendor/humbug/php-scoper/bin/php-scoper add-prefix --prefix=Plugin_NameVendor --output-dir=./vendor_prefixed/dframe --config=config/php-scoper/dframe.inc.php --force --quiet",
            "php ./vendor/humbug/php-scoper/bin/php-scoper add-prefix --prefix=Plugin_NameVendor --output-dir=./vendor_prefixed/pwpf --config=config/php-scoper/pwpf.inc.php --force --quiet"
        ],
        "post-install-cmd": [
            "Plugin_Name\\OverloadClass::prefixDependencies",
            "rm --r vendor/bin/",
            "rm --r vendor/dframe/",
            "rm --r vendor/pwpf/",
            "rm --r vendor/humbug/",
            "rm --r vendor/jetbrains/",
            "rm --r vendor/nikic/",
            "rm --r vendor/ocramius/",
            "rm --r vendor/psr/",
            "rm --r vendor/symfony/",
            "composer du"
        ],
        "post-update-cmd": [
            "Plugin_Name\\OverloadClass::prefixDependencies",
            "rm --r vendor/bin/",
            "rm --r vendor/dframe/",
            "rm --r vendor/pwpf/",
            "rm --r vendor/humbug/",
            "rm --r vendor/jetbrains/",
            "rm --r vendor/nikic/",
            "rm --r vendor/ocramius/",
            "rm --r vendor/psr/",
            "rm --r vendor/symfony/",
            "composer du"
        ]
    }
}
